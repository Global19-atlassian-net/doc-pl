 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>mixed</type><methodname>call_user_method</methodname>
   <methodparam><type>string</type><parameter>nazwa_metody</parameter></methodparam>
   <methodparam><type>object</type><parameter role="reference">obiekt</parameter></methodparam>
   <methodparam choice="opt"><type>mixed</type><parameter>parametr</parameter></methodparam>
   <methodparam choice="opt"><type>mixed</type><parameter>...</parameter></methodparam>
  </methodsynopsis>
  <warning>
   <para>
    Funkcja <function>call_user_method</function> jest od PHP 4.1.0
    przestarza³a.
   </para>
  </warning>
 </refsect1>

 <refsect1 role="parameters"><!-- {{{ -->
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>nazwa_metody</parameter></term>
     <listitem>
      <para>
       Nazwa wywo³ywanej metody.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term>
      <parameter>obiekt</parameter>
     </term>
     <listitem>
      <para>
       <type>obiekt</type>, w którym wywo³ywana jest
